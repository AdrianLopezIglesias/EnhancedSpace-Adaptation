    <xs:attribute name="Name" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="ObjectiveSetGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="GuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="TooltipDescription" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="unbounded" name="OutcomeCompleted" type="OutcomeGuiElement" />
          <xs:element minOccurs="0" maxOccurs="unbounded" name="OutcomeFailed" type="OutcomeGuiElement" />
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Objective" type="ObjectiveGuiElement" />
        </xs:sequence>
        <xs:attribute name="Politics" type="xs:string" />
        <xs:attribute name="OnlyChoiceTooltipKey" type="xs:string" />
        <xs:attribute name="Team" type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OutcomeGuiElement">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute default="" name="Name" type="xs:string" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="ObjectiveGuiElement">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="Name" type="xs:string" />
        <xs:attribute default="false" name="HideProgressionFeedback" type="xs:boolean" />
        <xs:attribute default="Default" name="ProgressionFeedback" type="QuestProgressionFeedbackType" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:simpleType name="QuestProgressionFeedbackType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Default" />
      <xs:enumeration value="TeamContribution" />
      <xs:enumeration value="CompetitiveRank" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="TutorialGuiElement" nillable="true" type="TutorialGuiElement" />
  <xs:complexType name="TutorialGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="QuestGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="PopupTitle" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="unbounded" name="PopupPage" type="TutorialPage" />
        </xs:sequence>
        <xs:attribute default="false" name="HidePortrait" type="xs:boolean" />
        <xs:attribute default="BelowPinnedQuest" name="AnchorMode" type="TutorialPopupAnchorMode" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TutorialPage">
    <xs:sequence>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element minOccurs="0" maxOccurs="1" name="TutorialLevelPrerequisite" type="TutorialLevelPrerequisite" />
        <xs:element minOccurs="0" maxOccurs="1" name="GameSettingPrerequisite" type="GameSettingPrerequisite" />
        <xs:element minOccurs="0" maxOccurs="1" name="QuestStatePrerequisite" type="QuestStatePrerequisite" />
        <xs:element minOccurs="0" maxOccurs="1" name="TechnologyPrerequisite" type="TechnologyPrerequisite" />
        <xs:element minOccurs="0" maxOccurs="1" name="PathPrerequisite" type="PathPrerequisite" />
        <xs:element minOccurs="0" maxOccurs="1" name="InterpreterPrerequisite" type="InterpreterPrerequisite" />
      </xs:choice>
      <xs:element minOccurs="0" maxOccurs="1" name="Title" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="Description" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Highlight" type="Highlight" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Disabler" type="Disabler" />
    </xs:sequence>
    <xs:attribute default="false" name="HighlightLanes" type="xs:boolean" />
  </xs:complexType>
  <xs:complexType name="TutorialLevelPrerequisite">
    <xs:simpleContent>
      <xs:extension base="Prerequisite" />
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="Prerequisite">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute default="false" name="Inverted" type="xs:boolean" />
        <xs:attribute name="Flags" type="xs:string" />
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="GameSettingPrerequisite">
    <xs:simpleContent>
      <xs:extension base="Prerequisite" />
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="QuestStatePrerequisite">
    <xs:complexContent mixed="false">
      <xs:extension base="Prerequisite">
        <xs:attribute name="QuestDefinitionName" type="xs:string" />
        <xs:attribute name="QuestState" type="QuestState" use="required" />
        <xs:attribute default="" name="QuestStepName" type="xs:string" />
        <xs:attribute default="Completed" name="QuestStepState" type="QuestState" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="QuestState">
    <xs:restriction base="xs:string">
      <xs:enumeration value="NotStarted" />
      <xs:enumeration value="InProgress" />
      <xs:enumeration value="Completed" />
      <xs:enumeration value="Failed" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="PathValidityPrerequisite">
    <xs:simpleContent>
      <xs:extension base="Prerequisite" />
    </xs:simpleContent>
