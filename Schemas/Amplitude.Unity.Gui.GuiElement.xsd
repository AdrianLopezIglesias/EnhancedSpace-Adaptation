<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Datatable" />
  <xs:element name="GuiElement" nillable="true" type="GuiElement" />
  <xs:complexType name="GuiElement">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="Title" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="Description" type="xs:string" />
      <xs:element minOccurs="0" maxOccurs="1" name="Icons" type="GuiElementIconCollection" />
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="GuiElementIconCollection">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Icon" type="IconDefinition" />
    </xs:sequence>
    <xs:attribute name="DefaultSize" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="IconDefinition">
    <xs:complexContent mixed="false">
      <xs:extension base="ValueType">
        <xs:attribute name="Path" type="xs:string" />
        <xs:attribute name="Size" type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ValueType" />
  <xs:element name="ExtendedGuiElement" nillable="true" type="ExtendedGuiElement" />
  <xs:complexType name="ExtendedGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="GuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="SymbolString" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="Sounds" type="GuiElementSoundCollection" />
          <xs:element minOccurs="0" maxOccurs="1" name="TooltipElement" type="GuiTooltipElement" />
          <xs:element minOccurs="0" maxOccurs="unbounded" name="TemporaryEffectTooltipElement" type="GuiTemporaryEffectTooltipElement" />
          <xs:element minOccurs="0" maxOccurs="1" name="Color" type="XmlColor" />
          <xs:element minOccurs="0" maxOccurs="1" name="AltTitle" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="AltDescription" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="Movies" type="GuiElementMovieCollection" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GuiElementSoundCollection">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Sound" type="SoundDefinition" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SoundDefinition">
    <xs:attribute name="EventName" type="xs:string" />
    <xs:attribute default="false" name="IsMusic" type="xs:boolean" />
    <xs:attribute name="Type" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="GuiTooltipElement">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="EffectOverride" type="xs:string" />
    </xs:sequence>
    <xs:attribute default="false" name="Percent" type="xs:boolean" />
    <xs:attribute default="" name="PercentFormat" type="xs:string" />
    <xs:attribute default="false" name="Ignore" type="xs:boolean" />
    <xs:attribute default="-1" name="CustomTurnDuration" type="xs:int" />
    <xs:attribute default="" name="TargetPropertyNameOverride" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="GuiTemporaryEffectTooltipElement">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="1" name="EffectOverride" type="xs:string" />
    </xs:sequence>
    <xs:attribute default="" name="TemporaryEffectName" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="XmlColor">
    <xs:complexContent mixed="false">
      <xs:extension base="ValueType">
        <xs:attribute name="Alpha" type="xs:unsignedByte" use="required" />
        <xs:attribute name="Blue" type="xs:unsignedByte" use="required" />
        <xs:attribute name="Green" type="xs:unsignedByte" use="required" />
        <xs:attribute name="Red" type="xs:unsignedByte" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GuiElementMovieCollection">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Movie" type="MovieDefinition" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="MovieDefinition">
    <xs:complexContent mixed="false">
      <xs:extension base="ValueType">
        <xs:attribute name="Path" type="xs:string" />
        <xs:attribute name="Type" type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="AffinityGuiElement" nillable="true" type="AffinityGuiElement" />
  <xs:complexType name="AffinityGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="ExtendedGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="ModelPath" type="xs:string" />
        </xs:sequence>
        <xs:attribute default="0" name="PreloadCount" type="xs:int" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="ResourceGuiElement" nillable="true" type="ResourceGuiElement" />
  <xs:complexType name="ResourceGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="ExtendedGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="TargetEffect" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="HeroGuiElement" nillable="true" type="HeroGuiElement" />
  <xs:complexType name="HeroGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="GuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="ModelPath" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="HeroClassGuiElement" nillable="true" type="HeroClassGuiElement" />
  <xs:complexType name="HeroClassGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="ExtendedGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="AffectedBy" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="PoliticsGuiElement" nillable="true" type="PoliticsGuiElement" />
  <xs:complexType name="PoliticsGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="ExtendedGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="ModelPath" type="xs:string" />
          <xs:element minOccurs="0" maxOccurs="1" name="PoliticsAffectingEvents" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="WindowGuiElement" nillable="true" type="WindowGuiElement" />
  <xs:complexType name="WindowGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="ExtendedGuiElement" />
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="LoadingWindowGuiElement" nillable="true" type="LoadingWindowGuiElement" />
  <xs:complexType name="LoadingWindowGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="WindowGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="TexturePath" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="MainMenuScreenGuiElement" nillable="true" type="MainMenuScreenGuiElement" />
  <xs:complexType name="MainMenuScreenGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="WindowGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Entry" type="MainMenuEntry" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="MainMenuEntry">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="SubEntry" type="MainMenuSubEntry" />
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Colored" type="xs:boolean" use="required" />
    <xs:attribute name="Enabled" type="xs:boolean" use="required" />
  </xs:complexType>
  <xs:complexType name="MainMenuSubEntry">
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Colored" type="xs:boolean" use="required" />
    <xs:attribute name="Enabled" type="xs:boolean" use="required" />
  </xs:complexType>
  <xs:element name="NewGameScreenGuiElement" nillable="true" type="NewGameScreenGuiElement" />
  <xs:complexType name="NewGameScreenGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="WindowGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Category" type="Category" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Category">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Entry" type="NewGameEntry" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="AdvancedSettingsGroup" type="AdvancedSettingsGroup" />
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute default="40" name="Width" type="xs:int" />
    <xs:attribute default="40" name="Height" type="xs:int" />
    <xs:attribute default="Standard" name="SettingStyle" type="SettingPrefabStyle" />
    <xs:attribute default="false" name="WithBanner" type="xs:boolean" />
  </xs:complexType>
  <xs:complexType name="NewGameEntry">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="ExtraValue" type="ExtraValue" />
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Type" type="ControlType" use="required" />
    <xs:attribute default="Invisible" name="VisibleInGame" type="InGameVisibility" />
    <xs:attribute name="Prefab" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="ExtraValue">
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Value" type="xs:string" />
  </xs:complexType>
  <xs:simpleType name="ControlType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Checkbox" />
      <xs:enumeration value="Droplist" />
      <xs:enumeration value="Slider" />
      <xs:enumeration value="Textfield" />
      <xs:enumeration value="Custom" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="InGameVisibility">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Invisible" />
      <xs:enumeration value="ReadOnly" />
      <xs:enumeration value="Editable" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="AdvancedSettingsGroup">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Entry" type="NewGameEntry" />
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" />
  </xs:complexType>
  <xs:simpleType name="SettingPrefabStyle">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Standard" />
      <xs:enumeration value="Compact" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ControlBannerGuiElement" nillable="true" type="ControlBannerGuiElement" />
  <xs:complexType name="ControlBannerGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="GuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="ControlBannerElement" type="ControlBannerElement" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ControlBannerElement">
    <xs:attribute name="Type" type="xs:string" />
    <xs:attribute name="WindowName" type="xs:string" />
  </xs:complexType>
  <xs:element name="TechnologyScreenGuiElement" nillable="true" type="TechnologyScreenGuiElement" />
  <xs:complexType name="TechnologyScreenGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="WindowGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="TechnologyQuadrantGuiElement" type="TechnologyQuadrantGuiElement" />
        </xs:sequence>
        <xs:attribute name="MaxTechnologyStages" type="xs:int" use="required" />
        <xs:attribute name="MinQuadrantRadius" type="xs:float" use="required" />
        <xs:attribute name="MaxQuadrantRadius" type="xs:float" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TechnologyQuadrantGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="ExtendedGuiElement">
        <xs:attribute name="CenterAngle" type="xs:float" use="required" />
        <xs:attribute name="VictoryStageStartAngle" type="xs:float" use="required" />
        <xs:attribute name="VictoryStageEndAngle" type="xs:float" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="TechnologyQuadrantGuiElement" nillable="true" type="TechnologyQuadrantGuiElement" />
  <xs:element name="EconomyScreenGuiElement" nillable="true" type="EconomyScreenGuiElement" />
  <xs:complexType name="EconomyScreenGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="WindowGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Section" type="Section" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Section">
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Category" type="xs:string" />
  </xs:complexType>
  <xs:element name="FleetsScreenGuiElement" nillable="true" type="FleetsScreenGuiElement" />
  <xs:complexType name="FleetsScreenGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="WindowGuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="FleetActionButton" type="FleetActionButtonItem" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FleetActionButtonItem">
    <xs:attribute name="ActionName" type="xs:string" />
    <xs:attribute name="ButtonType" type="xs:string" />
  </xs:complexType>
  <xs:element name="TechnologyGuiElement2" nillable="true" type="TechnologyGuiElement2" />
  <xs:complexType name="TechnologyGuiElement2">
    <xs:complexContent mixed="false">
      <xs:extension base="GuiElement">
        <xs:attribute name="X" type="xs:float" use="required" />
        <xs:attribute name="Y" type="xs:float" use="required" />
        <xs:attribute default="false" name="IsJuggernautTechnology" type="xs:boolean" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="TableGuiElement" nillable="true" type="TableGuiElement" />
  <xs:complexType name="TableGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="GuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Column" type="ColumnInfo" />
        </xs:sequence>
        <xs:attribute name="Height" type="xs:int" use="required" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ColumnInfo">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Filter" type="xs:string" />
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Description" type="xs:string" />
    <xs:attribute name="Width" type="xs:int" use="required" />
    <xs:attribute name="Prefab" type="xs:string" />
    <xs:attribute default="0" name="DigitsNumber" type="xs:int" />
    <xs:attribute default="false" name="Signed" type="xs:boolean" />
    <xs:attribute default="false" name="AutoResizeHeight" type="xs:boolean" />
    <xs:attribute default="0" name="FilterMenuWidth" type="xs:int" />
    <xs:attribute default="" name="FilterPropertyName" type="xs:string" />
    <xs:attribute default="false" name="DisableSorting" type="xs:boolean" />
  </xs:complexType>
  <xs:element name="LawGuiElement" nillable="true" type="LawGuiElement" />
  <xs:complexType name="LawGuiElement">
    <xs:complexContent mixed="false">
      <xs:extension base="GuiElement">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="1" name="ShortTitle" type="xs:string" />
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="QuestGuiElement" nillable="true" type="QuestGuiElement" />
