    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Type" type="xs:string" />
    <xs:attribute name="Persistent" type="xs:string" />
  </xs:complexType>
  <xs:complexType name="Disabler">
    <xs:attribute name="Name" type="xs:string" />
    <xs:attribute name="Disable" type="xs:boolean" use="required" />
  </xs:complexType>
