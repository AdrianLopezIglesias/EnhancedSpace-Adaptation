        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ArrayOfStepGuiElement">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Step" type="StepGuiElement" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="StepGuiElement">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="ObjectiveSet" type="ObjectiveSetGuiElement" />
