       <BinaryModifier TargetProperty="TechnologyCostQuadrantScienceAndExplorationMultiplierFromSpecialNodes"     Operation="Addition"   Left="0.002" BinaryOperation="Multiplication" Right="$(Multiplier)"/>
       <BinaryModifier TargetProperty="TechnologyCostQuadrantEconomyAndTradeMultiplierFromSpecialNodes"           Operation="Addition"   Left="0.002" BinaryOperation="Multiplication" Right="$(Multiplier)"/>
