    </SimulationDescriptor>
    
    <SimulationDescriptor Name="ModuleSupportJuggernautResearch" Type="ModuleSupportJuggernautResearch">
        <Property Name="NetEmpireResearchOnSpecialNodes"/>

        <Property Name="TechnologyCostQuadrantScienceAndExplorationMultiplierFromSpecialNodes" />
        <Property Name="TechnologyCostQuadrantEconomyAndTradeMultiplierFromSpecialNodes"       />
        <Property Name="TechnologyCostQuadrantEmpireDevelopmentMultiplierFromSpecialNodes"     />
        <Property Name="TechnologyCostQuadrantMilitaryMultiplierFromSpecialNodes"              />

        <!-- "I receive bonuses here". -->
        <Modifier TargetProperty="HasBonusesFromOrbit" Operation="Addition" Value="1" Path="../ClassGarrisonFleet"/>
        
        <!-- Bonus science (on any node) -->
        <Modifier TargetProperty="NetEmpireResearchOnSpecialNodes"    Operation="Addition"    Value="$(NetEmpireResearchOnSpecialNodes)"    Path="../ClassGarrisonFleet,FleetOrbitingSpecialNode"/>

        <!-- Black hole & neutron star -->
        <Modifier TargetProperty="TechnologyCostQuadrantScienceAndExplorationMultiplierFromSpecialNodes"     Operation="Subtraction"    Value="$(TechnologyCostQuadrantScienceAndExplorationMultiplierFromSpecialNodes)"     Path="../ClassGarrisonFleet,FleetOrbitingSpecialNodeBlackHole"/>
        <Modifier TargetProperty="TechnologyCostQuadrantScienceAndExplorationMultiplierFromSpecialNodes"     Operation="Subtraction"    Value="$(TechnologyCostQuadrantScienceAndExplorationMultiplierFromSpecialNodes)"     Path="../ClassGarrisonFleet,FleetOrbitingSpecialNodeNeutronStar" TooltipHidden="true"/>
        
        <!-- Asteroid field & solar nebula -->
        <Modifier TargetProperty="TechnologyCostQuadrantEconomyAndTradeMultiplierFromSpecialNodes"           Operation="Subtraction"    Value="$(TechnologyCostQuadrantEconomyAndTradeMultiplierFromSpecialNodes)"           Path="../ClassGarrisonFleet,FleetOrbitingSpecialNodeAsteroidField"/>
        <Modifier TargetProperty="TechnologyCostQuadrantEconomyAndTradeMultiplierFromSpecialNodes"           Operation="Subtraction"    Value="$(TechnologyCostQuadrantEconomyAndTradeMultiplierFromSpecialNodes)"           Path="../ClassGarrisonFleet,FleetOrbitingSpecialNodeSolarNebula" TooltipHidden="true"/>
        
        <!-- Space clouds & vaulters -->
        <Modifier TargetProperty="TechnologyCostQuadrantEmpireDevelopmentMultiplierFromSpecialNodes"         Operation="Subtraction"    Value="$(TechnologyCostQuadrantEmpireDevelopmentMultiplierFromSpecialNodes)"         Path="../ClassGarrisonFleet,FleetOrbitingSpecialNodeSpaceClouds"/>
        <Modifier TargetProperty="TechnologyCostQuadrantEmpireDevelopmentMultiplierFromSpecialNodes"         Operation="Subtraction"    Value="$(TechnologyCostQuadrantEmpireDevelopmentMultiplierFromSpecialNodes)"         Path="../ClassGarrisonFleet,FleetOrbitingSpecialNodeVaulters" TooltipHidden="true"/>
        
        <!-- Collapsing star -->
        <Modifier TargetProperty="TechnologyCostQuadrantMilitaryMultiplierFromSpecialNodes"                  Operation="Subtraction"    Value="$(TechnologyCostQuadrantMilitaryMultiplierFromSpecialNodes)"                  Path="../ClassGarrisonFleet,FleetOrbitingSpecialNodeCollapsingStar"/>

        <Modifier TargetProperty="JuggernautResearchModulesCount" Operation="Addition" Value="1" Path="../ClassEmpire"/>
    </SimulationDescriptor>

    <SimulationDescriptor Name="ModuleSupportJuggernautResearchStarting" Type="ModuleSupportJuggernautResearch">
