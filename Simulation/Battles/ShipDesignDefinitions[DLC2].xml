    <!-- Science juggernaut. -->
    <ShipDesignDefinition Name="Juggernauts_ScienceJuggernaut">
        <Tags>Science</Tags>
        <HullReference Name="HullJuggernaut01Juggernauts"/>
        <ShipRoleReference Name="ShipRoleJuggernaut"/>
        <!--Core Section-->
        <SlotBinding SlotName="CoreSection.Support01"> <!-- Support -->
            <ModuleReference Name="ModuleSupportJuggernautResearchStarting"/>
        </SlotBinding>
        <!--N Section-->
        <SlotBinding SlotName="SectionN.Support01"> <!-- def -->
            <ModuleReference Name="ModuleDefenseHullPlating1"/>
        </SlotBinding>
        <SlotBinding SlotName="SectionN.Weapon01"> <!-- att/supp -->
          <ModuleReference Name="ModuleSupportJuggernautResearchStarting"/>
        </SlotBinding>
        <!--NE Section-->
        <SlotBinding SlotName="SectionNE.Weapon01"> <!-- att/supp -->
          <ModuleReference Name="ModuleSupportJuggernautResearchStarting"/>
        </SlotBinding>
        <SlotBinding SlotName="SectionNE.Defense01"> <!-- def -->
            <ModuleReference Name="ModuleDefenseHullPlating1"/>
        </SlotBinding>
        <!--E Section-->
        <SlotBinding SlotName="SectionE.Weapon01"> <!-- att -->
          <ModuleReference Name="ModuleWeaponKinetic1"/>
        </SlotBinding>
        <!--SE Section-->
        <SlotBinding SlotName="SectionSE.Weapon01"> <!-- att -->
          <ModuleReference Name="ModuleWeaponKinetic1"/>
        </SlotBinding>
        <!--S Section-->

        <!-- If the ShipDesign is given by the FactionTraits, it will automatically appear in the MilitaryScreen when these prerequisites are validated. -->
        <TechnologyPrerequisite Flags="Prerequisite,Edition,Technology,Discard">TechnologyJuggernautUnlock</TechnologyPrerequisite>
    </ShipDesignDefinition>

