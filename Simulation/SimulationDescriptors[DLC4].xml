    <SimulationDescriptor Name="RelicSystemInteraction" Type="RelicSystemInteraction">

      <Property Name="EmpireIndustryRelics"            BaseValue="0" MinValue="0" IsSealed="true" />
      <Property Name="EmpireDustRelics"                BaseValue="0" MinValue="0" IsSealed="true" />
      <Property Name="EmpireFoodRelics"                BaseValue="0" MinValue="0" IsSealed="true" />
      <Property Name="EmpireInfluenceRelics"           BaseValue="0" MinValue="0" IsSealed="true" />
      
      <Property Name="EmpirePointProductionBoostMultiplier" BaseValue="0.15" MinValue="0"/>
      <Property Name="EmpireFoodProductionBoostMultiplier" BaseValue="0.15" MinValue="0"/>
      <Property Name="EmpireDustProductionBoostMultiplier" BaseValue="0.15" MinValue="0"/>
      <Property Name="EmpireIndustryProductionBoostMultiplier" BaseValue="0.15" MinValue="0"/>
      
      <Property Name="EmpirePointProductionBoost" BaseValue="0" MinValue="0"/>
      <Property Name="EmpireFoodProductionBoost" BaseValue="0" MinValue="0"/>
      <Property Name="EmpireDustProductionBoost" BaseValue="0" MinValue="0"/>
      <Property Name="EmpireIndustryProductionBoost" BaseValue="0" MinValue="0"/>
      
      <BinaryModifier TargetProperty="EmpirePointProductionBoost"       Operation="Addition" Left="$(EmpirePointProductionBoostMultiplier)" BinaryOperation="Multiplication" Right="$(EmpireInfluenceRelics)" />
      <BinaryModifier TargetProperty="EmpireFoodProductionBoost"        Operation="Addition" Left="$(EmpireFoodProductionBoostMultiplier)" BinaryOperation="Multiplication" Right="$(EmpireFoodRelics)" />
      <BinaryModifier TargetProperty="EmpireDustProductionBoost"        Operation="Addition" Left="$(EmpireDustProductionBoostMultiplier)" BinaryOperation="Multiplication" Right="$(EmpireDustRelics)" />
      <BinaryModifier TargetProperty="EmpireIndustryProductionBoost"    Operation="Addition" Left="$(EmpireIndustryProductionBoostMultiplier)" BinaryOperation="Multiplication" Right="$(EmpireIndustryRelics)" />

      <Modifier   TargetProperty="NetSystemEmpirePoint"   Operation="Percent" Value="$(EmpirePointProductionBoost)" TooltipHidden="false" Path="ClassEmpire/ClassColonizedStarSystem" ValueMustBePositive="true"/>
      <Modifier   TargetProperty="NetSystemGrowth"        Operation="Percent" Value="$(EmpireFoodProductionBoost)" TooltipHidden="false" Path="ClassEmpire/ClassColonizedStarSystem" ValueMustBePositive="true"/>
      <Modifier   TargetProperty="NetSystemMoney"         Operation="Percent" Value="$(EmpireDustProductionBoost)" TooltipHidden="false" Path="ClassEmpire/ClassColonizedStarSystem" ValueMustBePositive="true"/>
      <Modifier   TargetProperty="NetSystemProduction"    Operation="Percent" Value="$(EmpireIndustryProductionBoost)" TooltipHidden="false" Path="ClassEmpire/ClassColonizedStarSystem" ValueMustBePositive="true"/>

      <Modifier TargetProperty="NetEmpireRelics"  Operation="Addition"    Value="$(EmpireIndustryRelics)"       TooltipHidden="true"/>
      <Modifier TargetProperty="NetEmpireRelics"  Operation="Addition"    Value="$(EmpireDustRelics)"           TooltipHidden="true"/>
      <Modifier TargetProperty="NetEmpireRelics"  Operation="Addition"    Value="$(EmpireFoodRelics)"           TooltipHidden="true"/>
      <Modifier TargetProperty="NetEmpireRelics"  Operation="Addition"    Value="$(EmpireInfluenceRelics)"      TooltipHidden="true"/>

