      <Modifier TargetProperty="FIDIRelics"  Operation="Addition"    Value="$(EmpireFoodRelics)"          />
      <Modifier TargetProperty="FIDIRelics"  Operation="Addition"    Value="$(EmpireInfluenceRelics)"     />
     
